useDynLib(RobustBetaReg, .registration=TRUE)
exportPattern("^[[:alpha:]]+")
importFrom(Rcpp, evalCpp)

S3method(WaldTypeTest,LMDPDE)
S3method(WaldTypeTest,LSMLE)
S3method(cooks.distance,LMDPDE)
S3method(cooks.distance,LSMLE)
S3method(plot,LMDPDE)
S3method(plot,LSMLE)
S3method(plotenvelope,LMDPDE)
S3method(plotenvelope,LSMLE)
S3method(print,LMDPDE)
S3method(print,LSMLE)
S3method(print,WaldTest_LMDPDE)
S3method(print,WaldTest_LSMLE)
S3method(residuals,LMDPDE)
S3method(residuals,LSMLE)
S3method(robustbetareg.control,LMDPDE)
S3method(robustbetareg.control,LSMLE)
S3method(robustbetareg.control,default)
S3method(summary,LMDPDE)
S3method(summary,LSMLE)
export(WaldTypeTest)
export(degbeta)
export(hatvalues.robustbetareg)
export(plotenvelope)
export(regbeta)
export(robustbetareg)
export(robustbetareg.control)
export(soma)
